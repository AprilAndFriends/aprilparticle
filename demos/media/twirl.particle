<?xml version="1.0" encoding="UTF-8"?><ParticleSystem name="twirl">		<Space position="10,0,-10">		<Emitter name="left" blend_mode="add" emission_rate="100" limit="600" life="6" scale="0.3;0.5" direction="-0.5,0,0">			<Texture filename="red_particle"/>		</Emitter>		<!-- separate forces because of manual manipulation in code -->		<Affector name="force1" type="ForceField" radius="6" direction="0,0,0"/>		<Affector name="force2" type="ForceField" position="0,4,0" radius="6" direction="0,0,0"/>		<Affector reference="gravity1"/>		<Affector reference="gravity2"/>		<Affector reference="gravity3"/>	</Space>	<Space position="10,0,-10">		<Emitter name="right" blend_mode="add" emission_rate="100" limit="600" life="6" scale="0.3;0.5" direction="-0.5,0,0">			<Texture filename="green_particle"/>		</Emitter>		<!-- separate forces because of manual manipulation in code -->		<Affector name="force3" type="ForceField" radius="6" direction="0,0,0"/>		<Affector name="force4" type="ForceField" position="0,4,0" radius="6" direction="0,0,0"/>		<Affector reference="gravity1"/>		<Affector reference="gravity2"/>		<Affector reference="gravity3"/>	</Space>		<Affector name="gravity1" type="Attractor" position="-10,0,0" radius="15" force="4"/>	<Affector name="gravity2" type="Attractor" position="-5,0,5" radius="25" force="6"/>	<Affector name="gravity3" type="Attractor" position="-5,0,-5" radius="25" force="6"/>	</ParticleSystem>